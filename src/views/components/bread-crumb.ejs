<section class="bread-crumb" style="background-color: #e2d0c0">
  <div class="container">
    <div class="title-bread-crumb"><%= title %></div>
    <ul class="breadcrumb">
      <li class="home">
        <a href="/"><span>Home</span></a>
        <span class="mr_lr"
          >&nbsp;<svg
            aria-hidden="true"
            focusable="false"
            data-prefix="fas"
            data-icon="chevron-right"
            role="img"
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 320 512"
            class="svg-inline--fa fa-chevron-right fa-w-10">
            <path
              fill="currentColor"
              d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"
              class=""></path></svg
          >&nbsp;</span
        >
      </li>

      <% if (locals.title_menu) { %>
      <li>
        <a class="changeurl" href="/<%= toLowerCase(title_menu) %>">
          <span><%= title_menu %></span>
        </a>
        <span class="mr_lr"
          >&nbsp;<svg
            aria-hidden="true"
            focusable="false"
            data-prefix="fas"
            data-icon="chevron-right"
            role="img"
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 320 512"
            class="svg-inline--fa fa-chevron-right fa-w-10">
            <path
              fill="currentColor"
              d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"
              class=""></path></svg
          >&nbsp;</span
        >
      </li>
      <% } if (locals.category || locals.product) { %>
      <li>
        <a class="changeurl" href="/product"> <span>Product</span></a>
        <span class="mr_lr"
          >&nbsp;<svg
            aria-hidden="true"
            focusable="false"
            data-prefix="fas"
            data-icon="chevron-right"
            role="img"
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 320 512"
            class="svg-inline--fa fa-chevron-right fa-w-10">
            <path
              fill="currentColor"
              d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"
              class=""></path></svg
          >&nbsp;</span
        >
      </li>
      <% } if (locals.product) { %>
      <li>
        <a class="changeurl" href="<%= url %>"> <span><%= product.category %></span></a>
        <span class="mr_lr"
          >&nbsp;<svg
            aria-hidden="true"
            focusable="false"
            data-prefix="fas"
            data-icon="chevron-right"
            role="img"
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 320 512"
            class="svg-inline--fa fa-chevron-right fa-w-10">
            <path
              fill="currentColor"
              d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"
              class=""></path></svg
          >&nbsp;</span
        >
      </li>

      <li>
        <a class="changeurl" href="<%= `${url}&item=${urlItem}` %>"> <span><%= item %></span></a>
        <span class="mr_lr"
          >&nbsp;<svg
            aria-hidden="true"
            focusable="false"
            data-prefix="fas"
            data-icon="chevron-right"
            role="img"
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 320 512"
            class="svg-inline--fa fa-chevron-right fa-w-10">
            <path
              fill="currentColor"
              d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"
              class=""></path></svg
          >&nbsp;</span
        >
      </li>
      <% } if (locals.product) { %>
      <li>
        <strong><span><%= product.id %></span></strong>
      </li>
      <% } else if (locals.item) { %>
      <li>
        <a class="changeurl" href="<%= url %>"> <span><%= category %></span></a>
        <span class="mr_lr"
          >&nbsp;<svg
            aria-hidden="true"
            focusable="false"
            data-prefix="fas"
            data-icon="chevron-right"
            role="img"
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 320 512"
            class="svg-inline--fa fa-chevron-right fa-w-10">
            <path
              fill="currentColor"
              d="M285.476 272.971L91.132 467.314c-9.373 9.373-24.569 9.373-33.941 0l-22.667-22.667c-9.357-9.357-9.375-24.522-.04-33.901L188.505 256 34.484 101.255c-9.335-9.379-9.317-24.544.04-33.901l22.667-22.667c9.373-9.373 24.569-9.373 33.941 0L285.475 239.03c9.373 9.372 9.373 24.568.001 33.941z"
              class=""></path></svg
          >&nbsp;</span
        >
      </li>

      <li>
        <strong><span><%= item %></span></strong>
      </li>
      <% } else { %>
      <li>
        <strong><span><%= title %></span></strong>
      </li>
      <% } %>
    </ul>
    <div class="danhmuc-breadcrumb">
      <section class="section_danhmuc">
        <div class="container">
          <div class="danhmuc-swiper swiper-container">
            <div class="swiper-wrapper">
              <% const categoryData = [ { name: 'Agricultural', imageSrc:
              '/img/categories/category_1.jpg', url:'/product?category=agricultural' }, { name:
              'Wooden', imageSrc: '/img/categories/category_2.jpg', url: '/product?category=wooden'
              }, { name: 'Eco - Friendly handicrafts', imageSrc: '/img/categories/category_3.jpg',
              url: '/product?category=eco-friendly-handicrafts' }, { name: 'Rubber', imageSrc:
              '/img/categories/category_4.jpg', url: '/product?category=rubber' }, { name: 'Chilli',
              imageSrc: '/img/categories/category_5.jpg', url: '/product?category=chilli' }, ]; %>
              <% categoryData.forEach((categoryItem) => { %>
              <div class="swiper-slide">
                <a href="<%= categoryItem.url %>" title="<%= categoryItem.name %>"></a>
                <div class="danhmuc-thumb">
                  <img
                    width="200"
                    height="200"
                    class="lazyload"
                    src="<%= categoryItem.imageSrc %>"
                    alt="<%= categoryItem.name %>" />
                  <span class="image-shadow"></span>
                </div>
                <div class="danhmuc-info">
                  <h3><%= categoryItem.name %></h3>
                </div>
              </div>
              <% }); %>
            </div>
          </div>
        </div>
      </section>
      <script>
        var swiperdanhmuc = new Swiper('.danhmuc-swiper', {
          slidesPerView: 3,
          loop: false,
          grabCursor: true,
          roundLengths: true,
          slideToClickedSlide: false,
          slidesPerColumn: 1,
          spaceBetween: 30,
          autoplay: false,
          breakpoints: {
            300: {
              slidesPerView: 2,
            },
            500: {
              slidesPerView: 2,
            },
            640: {
              slidesPerView: 2,
            },
            768: {
              slidesPerView: 4,
            },
            991: {
              slidesPerView: 5,
            },
            1200: {
              slidesPerView: 5,
            },
          },
        })
      </script>
    </div>
  </div>
</section>
