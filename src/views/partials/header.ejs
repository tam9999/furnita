                  <a class="a-img a-index" href="/" title="Home"> Home </a>
                </li>

                <li class="nav-item <%= view_content.includes('about') ? 'active' : '' %>">
                  <a class="a-img a-index" href="/about" title="About"> About </a>
                </li>

                <li
                  class="nav-item has-mega <%= view_content.includes('product') ? 'active' : '' %>">
                  <a class="a-img caret-down a-index" href="/product" title="Products">
                    Products
                  </a>
                  <i class="fa fa-caret-down"></i>

                  <div class="mega-content d-lg-block d-none">
                    <div class="row">
                      <div class="col-lg-8 col-12">
                        <ul class="level0">
                          <% locals?.categoriesData.forEach((categoryItem) => { %>
                          <li
                            class="level1 parent item fix-navs"
                            data-title="<%= categoryItem.name %>"
                            data-link="<%= categoryItem.url %>">
                            <a
                              class="hmega"
                              href="<%= categoryItem.url %>"
                              title="<%= categoryItem.name %>">
                              <%= categoryItem.name %>
                            </a>
                            <% if (categoryItem.subcategories) { %>
                            <ul class="level1">
                              <% categoryItem.subcategories.forEach((subcategory) => { %>
                              <li class="level2">
                                <a href="<%= subcategory.url %>" title="<%= subcategory.name %>">
                                  <%= subcategory.name %>
                                </a>
                              </li>
                              <% }) %>
                            </ul>
                            <% } %>
                          </li>
                          <% }) %>
                        </ul>
                      </div>
                      <div class="col-lg-4 col-12">
                        <a href="/" title="Banner">
                          <img
                            width="414"
                            height="414"
                            class="lazyload"
                            style="border-radius: 5px"
                            src="/img/logo/lazy.png"
                            data-src="/img/categories/danhmuc_2.jpg"
                            alt="Banner" />
                        </a>
                      </div>
                    </div>
                  </div>
                  <ul class="item_small d-lg-none d-block">
                    <% locals?.categoriesData.forEach((categoryItem) => { %>
                    <li>
                      <a
                        class="caret-down"
                        href="<%= categoryItem.url %>"
                        title="<%= categoryItem.name %>">
                        <%= categoryItem.name %>
                      </a>
                      <% if (categoryItem.subcategories) { %>
                      <i class="fa fa-caret-down"></i>
                      <ul>
                        <% categoryItem.subcategories.forEach((subcategory) => { %>
                        <li>
                          <a
                            href="<%= subcategory.url %>"
                            title="<%= subcategory.name %>"
                            class="a3">
                            <%= subcategory.name %>
                          </a>
                        </li>
                        <% }) %>
                      </ul>
                      <% } %>
                    </li>
                    <% }) %>
