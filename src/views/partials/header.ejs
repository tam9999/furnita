<script>
  // Add a click event listener to hide various elements when clicked
  $(document).on(
    'click',
    '.backdrop__body-backdrop___1rvky, .cart_btn-close, .close-popup, .close-popup-search',
    function () {
      // Remove the 'active' class from multiple elements
      $(
        '.backdrop__body-backdrop___1rvky, .cart-sidebar, #popup-cart-desktop, .popup-cart-mobile, .popup-coupon, .popup-search'
      ).removeClass('active')

      // Remove 'search-active' class from the body element
      $('body').removeClass('search-active')

      // Empty the content of various elements
      $('.product-title, .product-new-price b, .noti-cart-count .count_item_pr').empty()

      // Remove the 'src' attribute from an image element
      $('.thumb-1x1 img').removeAttr('src')

      // Return false to prevent further propagation of the click event
      return false
    }
  )
</script>
